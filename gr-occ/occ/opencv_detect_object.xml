<?xml version="1.0"?>
<block>
  <name>detect_object</name>
  <key>opencv_detect_object</key>
  <category>opencv</category>
  <import>import opencv</import>
  <make>opencv.detect_object($cascadeFilename, $minFeatureSize, $scaledWidth)</make>
  <param>
    <name>Classifier XML File</name>
    <key>cascadeFilename</key>
    <value></value>
    <type>file_open</type>
  </param>
  <param>
    <name>Minimum Feature Size</name>
    <key>minFeatureSize</key>
    <value>20</value>
    <type>int</type>
  </param>
  <param>
    <name>Scaled Width</name>
    <key>scaledWidth</key>
    <value>320</value>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>
  <sink>
    <name>orig</name>
    <type>float</type>
  </sink>
  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
